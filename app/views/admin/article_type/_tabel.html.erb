<table class="table table-striped table-bordered tabelku">
	<thead>
		<tr>
			<th>No</th>
			<th>Kode Tipe</th>
			<th>Tipe Artikel</th>
			<th>Dibuat Oleh</th>
			<th>Waktu Dibuat</th>
			<th>Terakhir Diperbaharui</th>
			<th colspan="2" style="text-align: center">Operasi</th>			
		</tr>
	</thead>
	<tbody>
		
		<% @types.each_with_index do |key, hh| %>
		  <tr>
		  	<td><%= (@hhh.to_i + hh + 1) %></td>
		    <td><%= key.type_code %></td>
		    <td><%= key.article_type %></td>
		    <td><%= key.t_user.nme  %></td>
		    <td><%= key.created_at %></td>
		    <td><%= key.updated_at %></td>
		    <td class="lable-icon" title="Modifikasi"><%= link_to "i","/" + key.id.to_s + "/edit_keygroup" %></td>			
			<td class="lable-icon" title="Hapus"><%= link_to "t","/" + key.id.to_s + "/delete_keygroup", :confirm => 'Anda yakin akan menghapus data ini? Data tidak bisa dikembalikan.' %></td>
		  </tr>
		<% end %>
	</tbody>
	</table>
	<%= will_paginate @types, :id => "ajax_paginate" %>
  
