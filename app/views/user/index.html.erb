<nav class="navbar" role="navigation">
	<div class="navbar-header"><a class="navbar-brand" href="#">Form Isian<%= @nama_form %></a></div>
</nav>
<% if flash[:notice_success] %>
	<div class="alert alert-success"><%= flash[:notice_success] %></div>
<% end %>
<% if flash[:notice_failed] %>
	<div class="alert alert-danger"><%= flash[:notice_failed] %></div>
<% end %>
<%= form_tag({:action => @aksi_form},:class => "form-horizontal", :role => "form", :id => "form_isian") do %>
  <!-- Isian -->
  <div class="form-group">
    <label for="fullname" class="col-lg-2 control-label">Nama Lengkap</label>
    <div class="col-lg-4">
    	<%= 
    	 	text_field_tag(:fullname,@aa,:class => "form-control", :placeholder => "Shakira Rahmah", :id => "fullname") 
    	%>
      <span class="help-block keterangan">Maksimal 25 karakter dan hanya menggunakan karakter huruf.</span>
    </div>
    <div class="col-lg-4" id="v_fullname"></div>
  </div>
  <!-- Isian -->
  <div class="form-group">
    <label for="name_user" class="col-lg-2 control-label">Nama Pengguna</label>
    <div class="col-lg-4">
    	<%= 
    	 	text_field_tag(:name_user,@bb,:class => "form-control", :placeholder => "shakira_r", :id => "name_user") 
    	%>
      <span class="help-block keterangan">Maksimal 25 karakter dan hanya menggunakan karakter huruf, angka, dan underscore.</span>
    </div>
    <div class="col-lg-4" id="v_name_user"></div>
  </div>
  <!-- Isian -->
  <div class="form-group">
    <label for="password_user" class="col-lg-2 control-label">Password</label>
    <div class="col-lg-4">
    	<%= 
    	 	password_field_tag(:password_user, @cc, :class => "form-control", :id => "password_user") 
    	%>
    </div>
    <div class="col-lg-4" id="v_password_user"></div>
  </div>
  <!-- Isian -->
  <div class="form-group">
    <label for="password_user_b" class="col-lg-2 control-label"></label>
    <div class="col-lg-4">
    	<%= 
    	 	password_field_tag(:password_user_b, @cc, :class => "form-control", :id => "password_user_b",:placeholder => "Ketik ulang password.") 
    	%>
    	<span class="help-block keterangan">Maksimal 25 karakter dan hanya menggunakan karakter huruf dan angka.</span>
    </div>
    <div class="col-lg-4" id="v_password_user_b"></div>
  </div>
  <!-- Isian -->
  <div class="form-group">
    <label for="emaiL_address" class="col-lg-2 control-label">Email</label>
    <div class="col-lg-4">
    	<%= 
    	 	email_field(:emaiL_address,@dd,:class => "form-control", :placeholder => "shakirar@gmail.com", :id => "emaiL_address") 
    	%>
      <span class="help-block keterangan">Gunakan email yang aktif.</span>
    </div>
    <div class="col-lg-4" id="v_emaiL_address"></div>
  </div>
  <!-- Isian -->
  <div class="form-group">
    <label for="gender" class="col-lg-2 control-label">Jenis Kelamin</label>
    <div class="col-lg-4">
    	<%= select_tag(:gender, options_for_select([['Ikhwan',1],['Akhwat',2]]), :class => "form-control", :id => "gender") %>
    </div>
  </div>
  <!-- Isian -->
  <div class="form-group">
    <label for="organisasi" class="col-lg-2 control-label">Organisasi</label>
    <div class="col-lg-4">
      <%= select_tag(:persona, options_from_collection_for_select(@records, :id, :gname,'2')) %>

      <span class="help-block keterangan">Gunakan email yang aktif.</span>
    </div>
    <div class="col-lg-4" id="v_emaiL_address"></div>
  </div>
  <div class="form-group">
    <div class="col-lg-offset-2 col-lg-10 btn-group">
      <button type="button" class="btn btn-default" id="clear_form">Bersihkan</button>
      <button type="submit" class="btn btn-default" id="submit_btn"><%= @nama_btn %></button>
    </div>
  </div>	
<% end %>
<nav class="navbar" role="navigation">
	<div class="navbar-header">
    <a class="navbar-brand" href="#">Tabel Pengguna</a>
  </div>
  
  <%= form_tag({:action => "search"},:class => "navbar-form navbar-right", :role => "search", :id => "form_pencarian") do %>
  <div class="form-group">
        <%= text_field_tag(:search_form,"",:class => "form-control", :placeholder => "Cari nama organisasi...") %>
      </div>
        <button type="submit" class="btn btn-default">Submit</button>
    <% end %>
</nav>


<div id="table_itsar">
	<%= render partial: 'tabel' %>
</div>

<%= javascript_include_tag "itsar_group/script.js.erb" %>